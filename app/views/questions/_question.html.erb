<%= render '/layouts/errors', object: @question %>

<%= form_for @question, remote: true do |q| %>
  <%= q.label :title %>
  <%= q.text_field :title %>
  </br>
  <%= q.label :body %>
  <%= q.text_field :body %>
  </br>
  <%= q.hidden_field :user_id, value: current_user.id %>
  <%= q.submit "Submit" %>
<% end %>
